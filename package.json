{
  "name": "@solana-developers/helpers",
  "version": "2.5.2",
  "description": "Solana helper functions",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "type": "module",
  "private": false,
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./node": {
      "import": "./dist/esm/node.js",
      "require": "./dist/cjs/node.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "clean": "rm -rf dist",
    "test": "npm run test:ts && npm run test:anchor",
    "test:anchor": "cd tests/bankrun_test && anchor test",
    "test:ts": "jest --config jest.config.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "solana",
    "blockchain",
    "secret key",
    "private key",
    "keypair",
    "devnet SOL",
    "faucet"
  ],
  "author": "Mike MacCana <mike.maccana@solana.org>",
  "contributors": [
    "Nick Frostbutter",
    "John Liu",
    "Steven Luscher",
    "Christian Krueger",
    "Ayush Chauhan"
  ],
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@solana/spl-token": "^0.4.8",
    "@solana/spl-token-metadata": "^0.1.4",
    "@solana/web3.js": "^1.95.2",
    "@types/bn.js": "^5.1.5",
    "anchor-bankrun": "^0.4.0",
    "bn.js": "^5.2.1",
    "bs58": "^6.0.0",
    "dotenv": "^16.4.5",
    "solana-bankrun": "^0.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.16.1",
    "c8": "^10.1.2",
    "esrun": "^3.2.26",
    "prettier": "^3.0.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
